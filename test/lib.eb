(render-frame arg)
do  clear-background color-rgb 0 0 0 
do  draw-text
        color-rgb 255 255 255 
        pos-xy 10 10
        num-to-str arg
        48
    add 1 arg

(main)
do  open-window 512 512
do  loop do
        event-loop render-frame
        window-should-close
    0