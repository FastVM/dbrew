(is-prime-from x n)
if  lt mul n n x
    1
    if  mod n x
        is-prime-from x add 2 n
        0

(is-prime x)
if  mod 2 x
    is-prime-from x 3
    0

(loop n res)
let tmp if is-prime n n 0
    if  lt 1000000 n
        loop add 2 n add tmp res
        res

(main)
