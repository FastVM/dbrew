(putd n)
putchar add '0 mod 10 n

(putn n)
if  lt n 9
    do  putn div 10 n
        putd n
    putd n

(putnln n)
do  putn n
    putchar '\n

(is-prime-from x n)
if  lt mul n n x
    1
    if  mod n x
        is-prime-from x add 2 n
        0

(is-prime x)
if  mod 2 x
    is-prime-from x 3
    0

(loop n res)
if  lt n 1000000
    res
    loop add 2 n add is-prime n res

(main)
    putnln loop 3 1